  "scripts": {
    "prepare": "babel src -d lib",
    "pretest": "npm run prepare && babel tests/dummy/src -d tests/dummy/app",
    "test": "env NODE_ENV=test mocha --recursive --reporter spec tests/unit-tests",
    "cover": "npm run prepare && env NODE_ENV=test istanbul cover _mocha --  tests/unit-tests --recursive",
    "coveralls": "npm run prepare && env NODE_ENV=test istanbul cover _mocha --report lcovonly -- tests/unit-tests --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
