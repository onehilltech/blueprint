{
  "name": "@onehilltech/blueprint-dfinity",
  "description": "Blueprint module for integrating with the Internet Computer",
  "version": "5.0.0-alpha.6",
  "private": true,
  "main": "./lib",
  "scripts": {
    "prepare": "babel src -d app",
    "pretest": "npm run prepare && babel tests/dummy/src -d tests/dummy/app",
    "test": "env NODE_ENV=test mocha tests/unit-tests --recursive",
    "cover": "env NODE_ENV=test istanbul cover _mocha tests/unit-tests --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@dfinity/agent": "^0.11.1",
    "@dfinity/candid": "^0.11.1",
    "@dfinity/identity": "^0.11.1",
    "@dfinity/principal": "^0.11.1",
    "@onehilltech/blueprint": "^5.0.0-alpha.6",
    "bip39": "^3.0.4",
    "hdkey": "^2.0.1",
    "node-fetch": "^2.6.7",
    "p12-pem": "^1.0.5",
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@onehilltech/blueprint-cli": "^5.0.0-alpha.1",
    "@onehilltech/blueprint-testing": "^5.0.0-alpha.1"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "blueprint",
    "blueprint-module",
    "dfinity",
    "internet computer"
  ]
}
